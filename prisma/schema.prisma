// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model SimulationResult {
  id        Int      @id @default(autoincrement())
  user_id Int
  user User @relation(fields: [user_id], references: [id])
  name String
  propertyPrice Int
  property_tax_value Decimal //固定資産税評価額
  monthly_rent_income  Decimal //月額家賃収入
  montthly_management_fee Decimal //月額管理費
  annual_property_tax Decimal //固定資産税
  urban_planninng_tax Decimal //都市計画税
  net_rent_income Decimal //実質家賃収入
  net_yield Decimal //実質利回り
  updated_at DateTime? @updatedAt
  createdAt DateTime @default(now())
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime? @updatedAt
  SimulationResult SimulationResult[]
}